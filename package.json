{
  "name": "obs-developer-util",
  "displayName": "obs-developer-util",
  "description": "use vscode drive obs.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "obs-developer-util.helloWorld",
        "title": "Hello Status Bar"
      },
      {
        "command": "obs-developer-util.helloBar",
        "title": "Hello Bar"
      }
    ],
    "statusBarItems": [
      {
        "command": "obs-developer-util.statusBar",
        "text": "$(megaphone) line(s) selected",
        "alignment": "right"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.0.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.11.36",
    "@types/vscode": "^1.67.0",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.4.6",
    "eslint-plugin-prettier": "^4.0.0",
    "glob": "^8.0.1",
    "jest": "^27.2.0",
    "mocha": "^9.2.2",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "rxjs": "^7.5.5",
    "ws": "^8.7.0"
  }
}
